{
  "name": "passwall",
  "version": "1.2.1",
  "author": "PassWall <hello@passwall.io>",
  "description": "PassWall Desktop",
  "license": "MIT",
  "main": "./dist/main/index.js",
  "scripts": {
    "dev": "concurrently -k \"pnpm:dev:renderer\" \"pnpm:dev:main\" \"pnpm:dev:electron\"",
    "dev:renderer": "vite",
    "dev:main": "node scripts/watch-main.js",
    "dev:electron": "wait-on tcp:5174 && cross-env VITE_DEV_SERVER_URL=http://127.0.0.1:5174 electron .",
    "build": "pnpm run build:renderer && pnpm run build:main && electron-builder",
    "build:renderer": "vite build",
    "build:main": "node scripts/build-main.js",
    "ci:build:win:ia32": "pnpm run build:renderer && pnpm run build:main && electron-builder --win msi --ia32 --publish never",
    "ci:build:win:x64": "pnpm run build:renderer && pnpm run build:main && electron-builder --win msi --x64 --publish never",
    "ci:build:mac:x64": "pnpm run build:renderer && pnpm run build:main && electron-builder --mac dir --x64 --publish never",
    "ci:build:mac:arm64": "pnpm run build:renderer && pnpm run build:main && electron-builder --mac dir --arm64 --publish never",
    "ci:build:linux:x64": "pnpm run build:renderer && pnpm run build:main && electron-builder --linux deb rpm --x64 --publish never",
    "ci:build:linux:arm64": "pnpm run build:renderer && pnpm run build:main && electron-builder --linux deb rpm --arm64 --publish never",
    "format": "prettier --write \"src/**/*.{js,vue,json,css,scss,md}\" \"test/**/*.{js,vue,json,md}\" \"scripts/**/*.{js}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,json,css,scss,md}\" \"test/**/*.{js,vue,json,md}\" \"scripts/**/*.{js}\"",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "lint:ci": "eslint . && prettier --check .",
    "test": "pnpm run unit",
    "unit": "karma start test/unit/karma.conf.js"
  },
  "build": {
    "productName": "PassWall",
    "appId": "io.passwalll.desktop",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/main/**/*",
      "dist/renderer/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons/512x512.png",
      "target": [
        "deb"
      ],
      "category": "Development"
    }
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "@otplib/preset-browser": "^12.0.1",
    "axios": "^1.7.9",
    "papaparse": "^5.4.1",
    "vue": "^3.4.27",
    "vue-i18n": "^9.10.2",
    "vue-router": "^4.3.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.1.2",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/compiler-sfc": "^3.4.27",
    "concurrently": "^8.2.2",
    "electron": "^40.0.0",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.20.2",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^15.0.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-import": "^2.29.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^5.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "sass": "^1.77.5",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vite": "^5.4.6",
    "wait-on": "^7.2.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3",
    "prettier": "^3.3.3"
  },
  "packageManager": "pnpm@10.28.1"
}
